<view class="page-container">
    <view class="page-main">
        <view class="banner">
            <view class="banner-span">
                <view class="banner-span_icon" bindtap="pageclose"></view>
                <view class="banner-span_status">
                    <text>待确认</text>
                </view>
            </view>
            <view class="banner-content">
                <view class="banner-left">
                    <view class="banner-title">海珠65489船</view>
                    <view class="banner-describe">货物承运中</view>
                </view>
                <view class="banner-right">
                    <image src="/images/order/chuan.png"></image>
                </view>
            </view>
        </view>
        <view class="steps" wx:if="{{ status > 2 }}">
            <view class="steps-top">
                <view class="steps-summary">
                    <view class="steps-summary_icon">
                        <image src="/images/order/dww.png"></image>
                    </view>
                    <view class="steps-summary_info">
                        <view class="steps-summary_info_text">装好货</view>
                        <view class="steps-summary_info_describe">待船东上传</view>
                    </view>
                </view>
                <view class="steps-btu" wx:for="{{ stepsbtu }}" wx:key="index" wx:for-item="stepsbtuItem"
                    wx:if="{{ stepsbtuItem.show == true && userInfo.ship}}" data-state="{{stepsbtuItem.state}}"
                    bindtap="handleStepsBtu">
                    <text>{{ stepsbtuItem.title }}</text>
                </view>
                <view class="steps-btu" data-id="{{id}}" wx:if="{{userInfo.cargo}}" bindtap="handCargoStepsBtu">
                    <text>订单轨迹</text>
                </view>
                <!-- <view class="steps-btu" wx:if="{{ userInfo.ship && shipOrderInfo.transportStatus != 3 }}" bindtap="">
                    <text>上传装好货照片</text>
                </view> -->
            </view>
            <view class="steps-bottom">
                <vtu-steps value="{{value1}}" shape="spot" align-center>
                    <vtu-step wx:for="{{ steps }}" wx:key="index" wx:for-item="stepsItem" title="{{stepsItem.title}}"
                        status="{{stepsItem.status}}" active-color="{{stepsItem.color}}"></vtu-step>
                </vtu-steps>
            </view>
        </view>
        <view class="dialog-container">
            <!-- 船东确认订单金额弹框 -->
            <van-dialog use-slot title="确认订单金额吗？" show="{{ show }}" show-cancel-button show-confirm-button
                data-id="{{orderItem.id}}" bind:close="onClose" bind:confirm="handleConfirm">
            </van-dialog>

            <!-- 运输流程上传状态返回弹框 -->
            <van-dialog use-slot title="{{processtitle}}" show="{{ processShow }}" theme="round-button"
                confirm-button-text="我知道了">
                <view class="process_dialog_text">{{processtext}}</view>
                <view class="process_dialog_note" wx:if="{{shipOrderInfo.transportStatus === 2}}">{{processnote}}</view>
            </van-dialog>

            <!-- 确认价格弹框 -->
            <van-dialog use-slot show="{{ moneyShow1 }}" show-confirm-button="{{false}}">
                <view class="dialog-text">
                    <view class="dialog-subtitle">修改订单价格吗？</view>
                    <view class="dialog-msg">请输入本次订单最终签收货物吨数</view>
                </view>
                <view class="dialog-input">
                    <view class="dialog-input_item">
                        <vtu-input placeholder="请输入滞期费" value="{{delayedCost}}" type="input" text-align="left"
                            input-type="digit" bind:confirm="handledemurrage">
                            <view slot="after" class="amount">元</view>
                        </vtu-input>
                    </view>
                    <view class="dialog-input_item">
                        <vtu-input placeholder="请输入亏损扣费金额" value="{{loss}}" type="input" text-align="left"
                            input-type="digit" bind:confirm="handleloss">
                            <view slot="after" class="amount">元</view>
                        </vtu-input>
                    </view>
                    <view class="dialog-input_item">
                        <view class="subtitle">最终订单金额</view>
                        <view class="number">￥{{cargoOrderInfo.price}}</view>
                    </view>
                    <view class="dialog-input_item">
                        <vtu-input placeholder="可手动修改最终订单金额" value="{{orderPrice}}" type="input" text-align="left"
                            input-type="digit" bind:confirm="handleOrderPrice">
                            <view slot="after" class="amount">元</view>
                        </vtu-input>
                    </view>
                </view>
                <view class="dialog-btn">
                    <van-button type="{{moneybtn.type}}" round size="small" wx:for="{{moneyDialogBtn}}" wx:key="index"
                        wx:for-item="moneybtn" wx:if="{{moneybtn.show === true}}" custom-style="min-width: 96px;"
                        data-state="{{moneybtn.btnstate}}" bind:click="handledialogbtn">{{moneybtn.title}}</van-button>
                </view>
            </van-dialog>

            <!-- 余额不足弹框 -->
            <van-dialog use-slot show="{{ linesShow }}" show-confirm-button="{{false}}">
                <view class="dialog-text">
                    <view class="dialog-subtitle">余额不足以支付本次订单金额</view>
                    <view class="dialog-msg">请前往充值</view>
                </view>
                <view class="dialog-btn">
                    <van-button type="{{linesbtn.type}}" round size="small" wx:for="{{linesDialogBtn}}" wx:key="index"
                        wx:for-item="linesbtn" wx:if="{{linesbtn.show === true}}" custom-style="min-width: 96px;"
                        data-state="{{linesbtn.btnstate}}" bind:click="handlelinesbtn">{{linesbtn.title}}</van-button>
                </view>
            </van-dialog>

            <!-- 船东确认价钱弹框 -->
            <van-dialog use-slot show="{{ shipmoneyShow }}" show-confirm-button="{{false}}">
                <view class="dialog-text">
                    <view class="dialog-subtitle">同意确认运费总额吗？</view>
                    <view class="dialog-msg">本次最终运单金额</view>
                    <view class="dialog-money">￥168400.00</view>
                </view>
                <view class="dialog-btn">
                    <van-button type="{{shipMoneybtn.type}}" round size="small" wx:for="{{shipMoneyDialogBtn}}" wx:key="index"
                        wx:for-item="shipMoneybtn" wx:if="{{shipMoneybtn.show === true}}" custom-style="min-width: 96px;"
                        data-state="{{shipMoneybtn.btnstate}}" bind:click="shipConfirmOrderPrice">{{shipMoneybtn.title}}</van-button>
                </view>
            </van-dialog>

        </view>

        <!-- 货主订单信息盒 -->
