.tabs-item {
    margin: 3% 0;
    background: #FFFFFF;
    border-radius: 10px;
    padding: 3% 5%;
    border-radius: 3%;
}

/* 货主 开始 */
.tabs-time {
    font-size: 12px;
    color: #666666;
    margin-bottom: 2%;
}

.tabs-place {
    font-size: 14px;
    color: #191919;
    margin-bottom: 3%;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.tabs-goods {
    display: flex;
    margin-bottom: 3%;
}

.tabs-type {
    width: 45px;
    height: 20px;
    background: #F06622;
    border-radius: 10px;
    color: #FFFFFF;
    font-size: 12px;
    text-align: center;
    line-height: 18px;
}

.tabs-tonnage {
    font-size: 14px;
    color: #1F84E3;
    margin-left: 4%;
}

.tabs-ship-name {
    font-size: 12px;
    color: #1F84E3;
    margin-bottom: 2%;
}

.tabs-btnBox {
    padding-top: 3%;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    /* float: right; */
}

.order-msg {
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #F0222F;
    font-size: 13px;
    margin-right: 15px;
}

/* 货主结束 */

.quang {
    position: absolute;
    width: 80px;
    height: 80px;
    background: #FFFFFF;
    border: 2px dashed #E5E5E5;
    border-radius: 50%;
    font-size: 16px;
    font-family: PingFang SC;
    font-weight: 500;
    color: #BBBBBB;
    right: 21px;
    margin-top: -49px;

}

.quang_a {
    width: 60px;
    height: 60px;
    background: #FFFFFF;
    border: 2px dashed #E5E5E5;
    line-height: 57px;
    text-align: center;
    margin: 7px 8px;
    border-radius: 50%;
    transform: rotate(-45deg);

}

.tabs-place_item{
    display: flex;
    align-items: center;
}
.place-icon{
    color: #FFFFFF;
    font-family: PingFang-SC-Bold;
    font-weight: bold;
    font-size: 10px;
    width: 12px;
    height: 12px;
    text-align: center;
    line-height: 12px;
    margin-right: 5px;
}
.place-icon.start{
    background: #E3211F;
}
.place-icon.end{
    background: #369D40;
}
.place-subTitle{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    font-size: 12px;
    color: #191919;
    max-width: 100px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}
.place-img{

}
.place-img image{
    width: 55px;
    height: 7.5px;
}