        <text>是</text>
      </view>
    </view>
    <view class="info-item">
      <view class="subtitle">
        <text>开票资格有没</text>
      </view>
      <view class="text">
        <text>是</text>
      </view>
    </view>
    <view class="info-item">
      <view class="subtitle">
        <text>开票分数</text>
      </view>
      <view class="text">
        <text>是</text>
