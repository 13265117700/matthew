
.wharf-container{
    height: 100vh;
    display: flex;
    flex-direction: column;
}
.page-main{
    flex: 1;
    background: #ffffff;
}
.region-container{
    height: 100%;
    display: flex;
    flex-direction: column;
}
.region-list{
    flex: 1;
    padding: 0 20px;
    line-height: 35px;
    margin-top: 31px;
}
.region-title{
    font-family: PingFang-SC-Bold;
    font-weight: Bold;
    font-size: 16px;
    color: #191919FF;
    padding: 30px 20px;
}
.region-crumbs{
    font-family: region-crumbs;
    font-weight: Medium;
    color: #191919FF;
    font-size: 14px;
    padding: 0 20px;
    display: flex;
    align-items: center;
}
.region-crumbs_item{
    margin-right: 37px;
    position: relative;
}
.region-crumbs_item.active::after{
    content: '';
    position: absolute;
    width: 100%;
    height: 2px;
    bottom: -7px;
    left: 50%;
    transform: translateX(-50%);
    background: #E3211FFF;
}
.region-item{
    display: flex;
}
.region-item-img{
    margin-right: 10px;
}
.region-item-img image{
    width: 12px;
    height: 12px;
}
.region-cell{
    border: 1px solid #f1f1f1;
    border-radius: 10px;
    padding: 16px 16px !important; 
}
.popup-input{
    margin: 20px 12.5px;
    border: 1px solid #0A6EE8;
    border-radius: 9px;
}
.popup-input_label{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #191919FF;
    font-size: 17px;
}
.popup-input_text{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #999999FF !important;
    font-size: 14px;
}