.page-item{
    position: relative;
    margin: 10px 0;
}
.page-item image{
    width: 20px;
    height: 20px;
    margin-right: 10px;
}
.item-status{
    position: absolute;
    width: 55px;
    height: 26px;
    right: 7%;
    top: 7%;
}

.info-item{
    display: flex;
    justify-content: space-between;
    font-size: 14px;
    line-height: 30px;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
}
.info-name{
    color: #878787;
}
.info-tag{
    color: #191919;
}