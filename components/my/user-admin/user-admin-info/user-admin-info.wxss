/* components/my/user-admin/user-admin-info/user-admin-info.wxss */
.wrap-item{
    margin-top: 10px;
}
.Vtuzx_Input_Group_Center {
    border-radius: 10px;
}
.wrap-item-container{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 14.5px;
    height: 60px;
    line-height: 60px;
}
.item-subTitle{
    display: flex;
    /* justify-content: space-between; */
    align-items: center;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    font-size: 14px;
}
.item-subTitle_tag{
    color: #E3211FFF;
    padding-right: 10px;
}
.item-subTitle_text{
    color: #454545FF;
}
.item-input{
    display: flex;
    align-items: center;
    flex: 1;
    height: 60px;
    line-height: 60px;
    border-bottom: 1px solid #E8E8E8FF;
}
vtu-input{
    flex: 1;
}
.item-input_tag text{
    font-size: 14px;
    color: #191919FF;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
}
.item-input_tag image{
    width: 6.5px;
    height: 10.5px;
}

.wrap-item-container:last-child .item-input{
    border: none;
}

.wrap-item .item-title{
    display: flex;
    align-items: center;
    height: 52.5px;
    padding: 0 14.5px;
}
.wrap-item .item-title-tag{
    color: #E3211FFF;
    font-size: 14px;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    width: 30px;
    text-align: center;
    border-left: 4px solid #E3211FFF;
}
.wrap-item .item-iden{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 14.5px 20px;
}
.item-iden-image{
    width: 166px;
    height: 100px;
    background:#FFF4F4FF;
    border: 0.5px solid #E5E5E5FF;
    position: relative;
}
.item-iden-image image{
    width: 166px;
    height: 100px;
}
.item-iden-image .item-iden-image_icon{
    position: absolute;
    top: 30%;
    left: 50%;
    transform: translateX(-50%);
}
.item-iden-image .item-iden-image_icon image{
    width: 17px;
    height: 17px;
}
.item-iden-image .item-iden-image_text{
    position: absolute;
    bottom: 30%;
    left: 50%;
    transform: translateX(-50%);
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #191919FF;
    font-size: 12px;
    width: 84px;
}



.wrap-item .item-image{
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 86px;
    text-align: center;
}
.wrap-item .wrap-resource_add image{
    width: 35px;
    height: 30.5px;
}
.wrap-item .wrap-resource_add text{
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #666666FF;
    font-size: 12px;
}
.wrap-item .wrap-resource_add .item-video{
    width: 300px;
    margin: 0 auto;
}
.wrap-item_button{
    background: tomato;
    color: #ffffff;
    text-align: center;
    height: 50px;
    line-height: 50px;
    font-size: 18px;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    margin-top: 20px;
    cursor: pointer;
}